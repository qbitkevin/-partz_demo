{
  "name": "@partz/zdemo",
  "version": "0.8.2",
  "private": true,
  "description": "Demonstrate things in Partz - Not Published",
  "author": "ricks",
  "license": "UNLICENSED",
  "repository": "https://github.com/partially-artsy/partz/packages/zdemo",
  "keywords": [
    "demo", "partz", "artsy"
  ],
  "main": "index.js",
  "type": "module",
  "module": "zdemo.js",
  "sass": {
    "includePaths": ["./node_modules"]
  },
  "scripts": {
    "clean": "rm -rf ./dist/*",
    "unpackage": "rm -rf ./node_modules && rm -rf pnpm-lock.yaml",
    "reinstall": "pnpm install",
    "repackage": "pnpm run unpackage && pnpm run reinstall",
    "build:sass": "sass src/page.scss dist/page.css && sass src/ux-libs.scss dist/ux-libs.css",
    "build": "pnpm run build:sass && vite build && tsc && pnpm run build:sass",
    "preview": "vite preview",
    "test": "vitest",
    "dev": "pnpm run build:sass && vite"
  },
  "dependencies": {
    "@partz/abstract": "workspace:*",
    "@partz/alphabit": "workspace:*",
    "@partz/basics": "workspace:*",
    "@partz/time": "workspace:*",
    "@partz/conduit": "workspace:*",
    "@partz/piping": "workspace:*",
    "@partz/styling": "workspace:*",
    "@partz/ui-basics": "workspace:*",
    "@partz/ui-btn": "workspace:*",
    "@partz/ui-co": "workspace:*",
    "@partz/ui-form": "workspace:*",
    "@partz/ui-layout": "workspace:*",
    "@partz/ux-extent": "workspace:*",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "js-yaml": "^4.1.0"
  }
}
